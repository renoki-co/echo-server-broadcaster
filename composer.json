{
    "name": "renoki-co/echo-server-broadcaster",
    "description": "Echo Server Broadcaster is a Laravel Broadcaster used for Socket.IO-based Echo Server application.",
    "keywords": ["laravel", "php", "echo", "server", "socket", "socket.io", "websockets", "broadcast", "broadcasting"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/echo-server-broadcaster",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\EchoServerBroadcaster\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\EchoServerBroadcaster\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\EchoServerBroadcaster\\EchoServerBroadcasterServiceProvider"
            ]
        }
    }
}
