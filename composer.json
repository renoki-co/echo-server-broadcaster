{
    "name": "renoki-co/echo-server-core",
    "description": "Echo Server Core is a Laravel utility package used for Socket.IO-based Echo Server application.",
    "keywords": ["laravel", "php", "echo", "server", "socket", "socket.io", "websockets", "broadcast", "broadcasting"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/echo-server-core",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/broadcasting": "^7.29|^8.12",
        "illuminate/database": "^7.29|^8.12",
        "illuminate/routing": "^7.29|^8.12",
        "illuminate/support": "^7.29|^8.12",
        "pusher/pusher-php-server": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\EchoServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\EchoServer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.4",
        "orchestra/database": "^5.0|^6.0",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\EchoServer\\EchoServerServiceProvider"
            ]
        }
    }
}
